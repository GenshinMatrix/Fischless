<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:fui="urn:fischless.design"
                    xmlns:s="clr-namespace:System;assembly=mscorlib">

    <FontFamily x:Key="GenshinElementIcon">pack://application:,,,/Fischless;component/Assets/Fonts/GenshinElementIcon.ttf#GenshinElementIcon</FontFamily>
    <FontFamily x:Key="TeyvatBlack">pack://application:,,,/Fischless;component/Assets/Fonts/TeyvatBlack-Regular.ttf#Teyvat Black</FontFamily>
    <FontFamily x:Key="IcoMoon">pack://application:,,,/Fischless;component/Assets/Fonts/IcoMoon/fonts/icomoon.ttf#icomoon</FontFamily>

    <fui:UriToImageSourceConverter x:Key="UriToImageSourceConverter" />
    <BitmapImage x:Key="RarityBackground1" UriSource="{fui:Converter Value='pack://application:,,,/Fischless;component/Assets/Images/Rarity_1_Background.png', Converter={StaticResource UriToImageSourceConverter}}" />

</ResourceDictionary>
